include ../mixins/photoMixin

- console.log(main)
div(class='photos page')
    if main
        div(class='photos__title box-shadow layout_color_main')
            span(class='text text_color_normal text_uppercase text_size_1-2rem') #{title}
            i(class='link add-photo-button-js text text_color_normal text_size_2rem fas fa-plus-circle')   
        each photo in body
            + photoMixin(photo)
