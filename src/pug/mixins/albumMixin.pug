mixin albumMixin(data)
    - console.log(data)
    a(class="albums-card__link box-shadow" href='album/' + data.id section='album' + data.id)
        div(class="albums-card img" style="background-image: url('" + data.photo_url + "');")
            div(class="albums-card__body")
                span(class="text text_size_2rem text_color_for-buttons text_weight_bold albums-card__text") #{data.name}
