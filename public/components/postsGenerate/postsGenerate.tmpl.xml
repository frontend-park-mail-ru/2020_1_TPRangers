<fest:template xmlns:fest="http://fest.mail.ru" context_name="posts">
    <fest:for iterate="posts" index="i" value="post">
        <div class="postCard">
            <span class="postName"><fest:value>post.name</fest:value></span>
            <div class="postCardContent">
                <img class="image" src="<fest:value>post.imageData</fest:value>"/>
                <p class="postText"><fest:value>post.textData</fest:value></p>
            </div>
        </div>
    </fest:for>
</fest:template>